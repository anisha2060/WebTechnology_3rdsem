<?xml version="1.0" encoding="UTF-8">
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="students">
        <xs:complexType>
            <xs:all>
                <xs:element name="student">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="name">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="firstName" type:"xs:string" />
                                        <xs:element name="lastName" type:"xs:string" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="faculty" type:"xs:string" />
                            <xs:element name="batch" type:"xs:date" />
                             <xs:element name="contact">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="mobile" type:"xs:string" />
                                        <xs:element name="home" type:"xs:string" />
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            
                        </xs:sequence>
                        <xs:attribute name="id" type="xs:string" />
                    </xs:complexType>
                </xs:element>
            </xs:all>
            <xs:attribute name="college" type="xs:string" default="Nepathya" />
        </xs:complexType>
    </xs:element>
</xs:schema>

